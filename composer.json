{
  "name": "rollun-com/rollun-callback",
  "description": "Callback",
  "minimum-stability": "dev",
  "type": "library",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "avz-cmf",
      "email": "email@example.com"
    },
    {
      "name": "victorynox",
      "email": "it.proffesor02@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "aws/aws-sdk-php": "^2.8.31",
    "laminas/laminas-stdlib": "^3.7.0",
    "laminas/laminas-servicemanager": "^3.10",
    "laminas/laminas-http": "^2.15",
    "reputation-vip/queue-client": "^2.0",
    "opis/closure": "4.x-dev",
    "rollun-com/rollun-utils": "dev-laminas",
    "rollun-com/rollun-logger": "dev-laminas",
    "laminas/laminas-json": "3.4.x-dev",
    "laminas/laminas-stratigility": "3.6.x-dev",
    "laminas/laminas-diactoros": "2.9.x-dev",
    "laminas/laminas-db": "2.14.x-dev",
    "mtdowling/cron-expression": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "rollun\\callback\\": "src/Callback/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rollun\\test\\unit\\": "test/unit",
      "rollun\\test\\functional\\": "test/functional"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v",
    "code-sniffer": "phpcs .",
    "code-beautiful": "phpcbf .",
    "server": "php -S 0.0.0.0:8000 -t public public/index.php"
  },
  "repositories": [
    {
      "type": "path",
      "url": "/home/yurii/www/rollun/rollun-utils"
    },
    {
      "type": "path",
      "url": "/home/yurii/www/rollun/rollun-installer"
    },
    {
      "type": "path",
      "url": "/home/yurii/www/rollun/rollun-dic"
    },
    {
      "type": "path",
      "url": "/home/yurii/www/rollun/rollun-logger"
    }
  ],
  "config": {
    "allow-plugins": {
      "rollun-com/rollun-installer": true
    }
  }
}
